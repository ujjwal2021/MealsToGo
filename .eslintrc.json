{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        2,
        "double"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "max-len" : [
        "error",
        {"code": 200}
      ],
        "react-native/no-unused-styles": 2,
        "react/prop-types": 0,
        "no-unused-vars": 1,
        "react-native/split-platform-components": 2,
        "react-native/no-inline-styles": 2,
        "react-native/no-color-literals": 2,
        "react-native/no-raw-text": 2,
        "require-jsdoc" : "off",
        "valid-jsdoc": "off"

    }
}

